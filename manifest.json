{
  "name": "Vexor Unanimated Chrome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get rid of animations on Vexor",
  "content_scripts": [
    {
      "matches":  ["https://vexor-web.bebanjo.net/*"],
      "css":      ["assets/vexor.css"],
      "run_at":   "document_end",
      "all_frames": true
    }
  ]
}
